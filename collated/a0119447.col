	/**
	 * Check whether a task is done
	 *
	 * @return 	whether a task is done
	 */
	public boolean isDone() {
		if (this.status.equals(Constant.TASK_STATUS_DONE)) {
			return true;
		} else {
			return false;
		}
	}
	
